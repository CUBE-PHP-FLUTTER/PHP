
    <h1>{{ telephone.titre }}</h1>
    <p><strong>Description :</strong>{{ telephone.description }}</p>
    <p><strong>Auteur :</strong>{{ telephone.Auteur }}</p>
    <p><strong>Date :</strong>{{ telephone.DatePublication|date("d/m/Y") }}</p>
    <p>

        {% if file_exist( './uploads/images/'~telephone.ImageRepository~'/'~telephone.ImageFileName ) and telephone.ImageFileName !="" %}

            <img src="/uploads/images/{{ telephone.ImageRepository }}/{{ telephone.ImageFileName }}" class="img-thumbnail"/>


        {% endif %}

    </p>
